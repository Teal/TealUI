{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "启动",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/test/basic.js",
			"stopOnEntry": false,
			"args": [],
			"timeout": 30000,
			"cwd": "${workspaceRoot}/test",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/lib"
		},
		{
			"name": "附加",
			"type": "node",
			"request": "attach",
			"sourceMaps": true,
			"timeout": 30000,
			"outDir": "${workspaceRoot}/lib",
			"port": 5858
		}
	]
}